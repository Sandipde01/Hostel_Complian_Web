       <section class="table__header">
           <h1>Complaint Table</h1>
       </section>
       <section class="table__body">

           <table>
               <thead>
               <tr>
                   <th> Student id </th>
                   <th> Room No </th>
                   <th> Phone No </th>
                   <th> Complaint Date </th>
                   <th> Complaint Type </th>
                   <th> Description </th>
                   
                  
               </tr>
           </thead>
           <tbody>
              <% for(data of newData){ %>

                <% for(complain of data.complaine ){ %>

                    <tr> <td> <%=data.name  %> </td>
                        <td> <%=data.room  %> </td>
                        <td> <%=data.ph  %> </td>
                        <td> 22.04.23 </td>
                        <td><strong> <%=complain.type  %> </strong></td>
                        <td> <%=complain.description  %> </td>
                <% } %>
